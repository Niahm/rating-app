- var tab = (typeof navtab === 'undefined') ? "" : navtab;
!!! 5
html
  head
    title= title
    meta(name='viewport',content='width=device-width, initial-scale = 1.0, user-scalable = no')
    link(rel='stylesheet',href='/css/bootstrap.min.css')
    link(rel='stylesheet',href='/css/bootstrap-responsive.min.css')
    - if(developmod )
      link(rel='stylesheet/less', href='/css/share.less')
      script(src='/lib/less/1.2.1/less.min.js')
    - else
      link(rel='stylesheet', href='/css/share.css?t=#{timestamp}')
    script(src='/js/modules/seajs/1.1.1/sea.js')
    script(src='/lib/jquery/jquery-1.7.1.min.js')

  body
    include topbar
    div#main.container-fluid!= body
    include footer
    - if(tab === 'user')
      script(src="/bootstrap/js/bootstrap-alerts.js")
    - if(tab === 'sharewish')
      script(src="/js/share-wish/share-wish.js")
